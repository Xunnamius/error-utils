{
  "name": "named-app-errors",
  "version": "4.0.1",
  "description": "A handy set of NAMED (i.e. in the stack trace) error classes with deep TypeScript support and other useful additions",
  "keywords": [
    "app",
    "application",
    "error",
    "errors",
    "name",
    "named",
    "trace",
    "stack",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Xunnamius/named-app-errors"
  },
  "license": "MIT",
  "author": "Xunnamius",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/modules/index.d.ts",
      "import": "./dist/modules/index.js",
      "require": "./dist/umd.js",
      "default": "./dist/umd.js"
    },
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "main": "dist/umd.js",
  "module": "dist/modules/index.js",
  "types": "dist/modules/index.d.ts",
  "files": [
    "/dist",
    "/LICENSE",
    "/package.json",
    "/README.md"
  ],
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.types.json && NODE_ENV=production webpack && NODE_ENV=production babel src --extensions .ts --out-dir dist/modules",
    "check-types": "command -v unbuffer >/dev/null && (unbuffer tsc; unbuffer eslint .) | less -R -FX || (tsc; eslint .)",
    "clean": "rm -rf dist",
    "list-tasks": "npm run",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "npm run clean",
    "repl": "command -v rlwrap >/dev/null && rlwrap npx -p @babel/core -p @babel/node babel-node",
    "test": "BABEL_ENV=test jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/plugin-proposal-export-default-from": "^7.22.5",
    "@babel/plugin-proposal-function-bind": "^7.22.5",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-throw-expressions": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/confusing-browser-globals": "^1.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-source-map-support": "^2.2.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "^8.43.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-circus": "^29.5.0",
    "jest-extended": "^4.0.0",
    "source-map-support": "^0.5.21",
    "toss-expression": "^0.1.2",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4"
  }
}
